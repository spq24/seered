<!DOCTYPE html>
<html>
<head>
  <title>Seered</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
		<%= render 'layouts/header' %>
		    <% flash.each do |name, msg| %>
              <%= content_tag(:div, msg, class: "alert alert-#{name}") %>
            <% end %>
		<div class="body">
		<%= yield %>
		</div>
	</div>
</body>
</html>
